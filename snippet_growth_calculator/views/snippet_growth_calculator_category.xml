<odoo>
    <template id="snippet_growth_calculator_category" inherit_id="website.snippets" name="Growth Calculator Snippet Category">
        <xpath expr="//div[@id='snippets']" position="inside">
            <t t-call="website.snippet_options">
                <section class="o_snippet_body">
                    <t t-call="snippet_growth_calculator"/>
                </section>
            </t>
        </xpath>
    </template>
</odoo>
